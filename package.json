{
  "name": "critterbase-api",
  "version": "1.0.0",
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "description": "Critterbase API",
  "main": "./src/server.ts",
  "scripts": {
    "start": "npx prisma migrate deploy || npx pm2 --no-daemon start dist/src/server.js",
    "build": "tsc",
    "test": "jest --forceExit",
    "test:watch": "jest --watchAll --forceExit",
    "dev": "npx prisma generate && nodemon -r dotenv/config ./src/server.ts",
    "lint": "npx eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix --ignore-pattern 'node_modules' --ext .ts",
    "format:fix":"prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "format:check": "prettier --check \"./src/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "dead:code": "ts-prune | (! grep -v 'used in module')",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:dev-setup": "npx prisma migrate deploy && npx prisma db seed"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.0",
    "@types/nodemon": "^1.19.2",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "babel-jest": "^29.4.3",
    "eslint": "^8.37.0",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "prettier": "^3.2.5",
    "prisma": "^4.11.0",
    "ts-prune": "^0.10.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.370.0",
    "@aws-sdk/s3-request-presigner": "^3.370.0",
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@prisma/client": "^4.11.0",
    "@swc/core": "^1.3.76",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-openapi": "^9.3.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.1",
    "jwks-rsa": "^3.0.1",
    "multer": "^1.4.5-lts.1",
    "pm2": "^5.2.2",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "ts-node": "^10.9.1",
    "zod": "^3.21.4",
    "zod-openapi": "^2.6.2"
  }
}
